{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "start": "deno run -A --watch=static/,routes/,components/,islands/ dev.ts",
    "dev": "export ENV=development && deno run -A --watch=static/,routes/,components/,islands/ dev.ts",
    "build": "deno run -A dev.ts build",
    "docs": "deno run --allow-read --allow-write swagger.js",
    "fakebuild": "echo 'fake build'",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "nodeModulesDir": true,
  "importMap": "./.vscode/import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}